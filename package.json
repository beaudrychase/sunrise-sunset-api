{
  "name": "sunrise-sunset-api",
  "version": "1.0.0",
  "description": "a simple api to get the relative position of the sun",
  "main": "dist/app.js",
  "directories": {
    "doc": "docs",
    "source": "src"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --build -p tsconfig.json",
    "run": "node dist/app.js",
    "clean": "tsc --build --clean -p tsconfig.json",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beaudrychase/sunrise-sunset-api.git"
  },
  "keywords": [],
  "author": "beaudrychase",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beaudrychase/sunrise-sunset-api/issues"
  },
  "homepage": "https://beaudrychase.xyz/sunrise-sunset-api/",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "eslint": "8.36.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "suncalc": "^1.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "testMatch": [
      "**/test/**/*.+(ts|tsx|js)"
    ]
  }
}
